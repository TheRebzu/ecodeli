    "test": "vitest",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "// === ANALYSE ===": "",
    "analyze": "ANALYZE=true npm run build",
    "analyze:bundle": "ANALYZE=true next build",
    "analyze:deps": "tsx scripts/diagnostics/analyze-dependencies.ts",
    "check:circular": "madge --circular --extensions ts,tsx src/",
    "check:unused": "ts-unused-exports tsconfig.json --allowUnusedTypes",
