{
  "name": "ecodeli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "update-deps": "ncu -u && npm install",
    "security-audit": "npm audit fix",
    "deploy:dev": "npm run docker:dev",
    "deploy:prod": "npm run k8s:deploy",
    "deploy:full": "npm run build && npm run docker:build && npm run k8s:deploy",
    "docker:build": "docker build -t ecodeli .",
    "docker:run": "docker run -p 3000:3000 ecodeli",
    "docker:rebuild": "npm run docker:build && npm run docker:run",
    "docker:dev": "docker-compose up --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up --build",
    "docker:stop": "docker-compose down",
    "docker:clean": "docker system prune -f && docker volume prune -f",
    "k8s:init": "kubectl create namespace ecodeli || true",
    "k8s:secrets": "kubectl apply -f k8s/base/secrets.yaml",
    "k8s:deploy": "kubectl apply -k k8s/base/",
    "k8s:delete": "kubectl delete -k k8s/base/",
    "k8s:rebuild": "npm run docker:build && npm run k8s:deploy",
    "k8s:logs": "kubectl logs -l app=ecodeli -n ecodeli",
    "k8s:status": "kubectl get all -n ecodeli",
    "k8s:port-forward": "kubectl port-forward service/ecodeli-service 3000:80 -n ecodeli",
    "k8s:rollback": "kubectl rollout undo deployment/ecodeli-app -n ecodeli",
    "k8s:update": "kubectl set image deployment/ecodeli-app ecodeli=ecodeli-app:latest",
    "k8s:scale": "kubectl scale deployment ecodeli-app --replicas=3 -n ecodeli",
    "k8s:debug": "kubectl describe deployment ecodeli-app -n ecodeli",
    "k8s:events": "kubectl get events -n ecodeli"
  },
