  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "prepare": "husky",
    "// Testing": "",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "// Database": "",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:seed:all": "npx tsx prisma/seeds/run-all-seeds.ts",
    "db:seed:enhanced": "npx tsx prisma/seeds/run-enhanced-seeds.ts",
    "db:seed:clean": "npx tsx prisma/seeds/run-all-seeds.ts --clean",
    "// I18n": "",
    "i18n:extract": "npx tsx scripts/i18n/run-extraction.ts",
    "i18n:generate": "npx tsx scripts/i18n/run-generation.ts",
    "i18n:validate": "npx tsx scripts/i18n/run-validation.ts",
    "i18n:full": "npx tsx scripts/i18n/run-workflow.ts",
    "// Docker": "",
    "docker:dev": "cd docker && docker-compose up -d",
    "docker:dev:down": "cd docker && docker-compose down",
    "docker:prod": "cd docker && docker-compose -f docker-compose.prod.yml up -d",
    "docker:prod:down": "cd docker && docker-compose -f docker-compose.prod.yml down",
    "// Analysis": "",
    "analyze:deps": "tsx scripts/analyze-dependencies.ts",
    "analyze:bundle": "ANALYZE=true next build",
    "fix:imports": "tsx scripts/fix-imports-auto.ts",
    "fix:imports:dry": "tsx scripts/fix-imports-auto.ts --dry-run",
    "fix:all-imports": "node scripts/fix-all-imports.cjs",
    "// Index Generation": "",
    "index:generate": "node scripts/generate-indexes.mjs",
    "index:clean": "node scripts/generate-indexes.mjs --clean",
    "index:rebuild": "node scripts/generate-indexes.mjs --clean && node scripts/generate-indexes.mjs"
  },
