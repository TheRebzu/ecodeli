    "test:pages": "tsx scripts/testing/pages-checker.ts",
    "test:pages:quick": "tsx scripts/testing/quick-page-test.ts",
    "test:announcements": "tsx scripts/testing/test-announcement-features.ts",
    "test:seeds": "tsx scripts/testing/test-verification-seeds.ts",
    "// === API TESTING ====": "",
    "test:api": "tsx scripts/api-test/index.ts",
    "test:api:auth": "tsx scripts/api-test/index.ts auth",
    "test:api:annonces": "tsx scripts/api-test/tests/annonces/annonces.test.ts",
    "test:api:scenario": "tsx scripts/api-test/scenarios/full-cycle.ts",
    "test:api:all": "tsx scripts/api-test/run-all-tests.ts",
    "// === MONITORING ====": "",
    "monitoring": "tsx scripts/monitoring/health-check.ts",
    "monitoring:quick": "bash scripts/monitoring/quick-health-check.sh",
    "// === API DOCUMENTATION ====": "",
    "docs:api:generate": "tsx scripts/add-api-docs.ts",
    "docs:api:check": "curl -s http://localhost:3000/api/openapi | jq '.paths | length'",
