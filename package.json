{
  "name": "ecodeli",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "pnpm run prisma:generate",
    "build": "next build",
    "build:fast": "NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production next build",
    "build:minimal": "NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production NEXT_SHARP_PATH=0 next build",
    "build:no-cache": "rm -rf .next && npm run build:fast",
    "build:fast-win": "set NEXT_TELEMETRY_DISABLED=1 && set NODE_ENV=production && next build",
    "build:minimal-win": "set NEXT_TELEMETRY_DISABLED=1 && set NODE_ENV=production && set NEXT_SHARP_PATH=0 && next build",
    "build:no-cache-win": "if exist .next rmdir /s /q .next && npm run build:fast-win",
    "postbuild": "node scripts/post-build.js",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "seed": "tsx prisma/seed.ts",
    "db:seed": "tsx prisma/seed.ts",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "fix-ts": "node fix-ts-simple.js && pnpm run type-check",
    "prisma:merge": "node prisma/merge-schemas.js",
    "prisma:generate": "pnpm run prisma:merge && npx prisma generate",
    "prisma:migrate": "pnpm run prisma:merge && npx prisma migrate deploy",
    "i18n:extract": "node scripts/extract-i18n-labels.js",
    "i18n:clean": "node scripts/clean-i18n.js",
    "i18n:fix-specific": "node scripts/fix-specific-keys.js",
    "i18n:fix-legal": "node scripts/fix-legal-keys.js",
    "i18n:fix": "pnpm run i18n:extract && pnpm run i18n:clean",
    "i18n:fix-all": "pnpm run i18n:extract && pnpm run i18n:fix-specific && pnpm run i18n:fix-legal && pnpm run i18n:clean",
    "i18n:build": "pnpm run i18n:fix-all && pnpm run build"
  },
