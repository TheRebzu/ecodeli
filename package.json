    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seeds/run-all-seeds.ts",
    "seed:all": "tsx prisma/seeds/run-all-seeds.ts",
    "seed:base": "tsx prisma/seeds/run-all-seeds.ts --scope=base",
