    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:clean": "node scripts/fixes/clean-lint-errors.js",
    "lint:clean:phase2": "node scripts/fixes/clean-lint-errors-phase2.js",
    "lint:fix:parsing": "tsx scripts/fixes/fix-parsing-errors-final.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "// === NETTOYAGE ===": "",
    "clean": "rm -rf .next dist build",
    "clean:deps": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "clean:build": "rm -rf .next dist build && pnpm db:generate",
    "clean:all": "pnpm clean && pnpm clean:deps",
    "clean:simulation": "node scripts/fixes/clean-simulation-references.js",
    "clean:mocks": "node scripts/fixes/clean-simulation-references.js",
