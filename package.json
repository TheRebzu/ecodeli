{
  "name": "ecodeli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "// === BUILD & DÉVELOPPEMENT ===": "",
    "dev": "next dev",
    "dev:next": "next dev",
    "dev:socket": "node socket-server.cjs",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:socket\"",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start",
    "// === CORRECTION AUTOMATIQUE ===": "",
    "fix": "pnpm run fix:imports && pnpm run fix:lint && pnpm run fix:format",
    "fix:build": "tsx scripts/fixes/fix-build-errors.ts",
    "fix:imports": "tsx scripts/fixes/fix-imports-auto.ts",
    "fix:exports": "tsx scripts/fixes/fix-missing-exports.ts",
    "fix:critical": "tsx scripts/fixes/fix-critical-errors.ts",
    "fix:lint": "pnpm lint --fix",
    "fix:format": "pnpm format",
    "fix:types": "tsx scripts/fixes/fix-build-errors.ts",
    "fix:batch": "tsx scripts/fixes/fix-all-missing-exports.ts",
    "fix:all": "pnpm clean:build && pnpm fix:critical && pnpm fix:exports && pnpm fix:batch && pnpm fix && pnpm build",
    "// === DIAGNOSTIC ===": "",
    "check": "tsx scripts/diagnostics/diagnostic-quick.ts",
    "check:fix": "tsx scripts/diagnostics/diagnostic-quick.ts --fix",
    "check:quick": "pnpm typecheck && pnpm lint && pnpm format:check",
    "check:full": "tsx scripts/diagnostics/check-all.ts --report",
