    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "commit": "git-cz",
    "// === DÉPLOIEMENT ===": "",
    "build:prod": "cross-env NODE_ENV=production pnpm build",
    "deploy:preview": "vercel",
    "deploy:prod": "vercel --prod",
