    "docker:dev": "./scripts/docker/deploy.sh dev",
    "docker:prod": "./scripts/docker/deploy.sh prod",
    "docker:stop": "./scripts/docker/deploy.sh stop",
    "docker:restart": "./scripts/docker/deploy.sh restart",
    "docker:logs": "./scripts/docker/deploy.sh logs",
    "docker:status": "./scripts/docker/deploy.sh status",
    "docker:backup": "./scripts/docker/backup.sh full",
    "docker:backup:quick": "./scripts/docker/backup.sh quick",
    "docker:health": "./scripts/docker/health-check.sh",
    "docker:cleanup": "./scripts/docker/deploy.sh cleanup",
    "// === UTILITAIRES ===": "",
    "fix:imports:dry": "tsx scripts/fixes/fix-imports-auto.ts --dry-run",
    "// === BUILD & GÉNÉRATION ===": "",
    "index:generate": "node scripts/build/generate-indexes.mjs",
    "index:clean": "node scripts/build/generate-indexes.mjs --clean",
    "index:rebuild": "node scripts/build/generate-indexes.mjs --clean && node scripts/build/generate-indexes.mjs",
